{
  "name": "symbolic-calculus",
  "version": "1.3.5",
  "description": "Solves Calculus based equations",
  "main": "build/index.js",
  "scripts": {
    "start": "nodemon --exec ./node_modules/.bin/ts-node -- src/index.ts",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "tsc",
    "build-continuous": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FermiDirak/symbolic-calculus.git"
  },
  "keywords": [
    "calculus",
    "solver"
  ],
  "author": "Bryan Manuele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FermiDirak/symbolic-calculus/issues"
  },
  "homepage": "https://github.com/FermiDirak/symbolic-calculus#readme",
  "devDependencies": {
    "@types/node": "^10.12.18",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$"
  }
}
